<table border="1">
    <tr>
        <th>Email</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Phone Number</th>
        <th>Gender</th>
        <th>Image</th>
        <th>Role ID</th>
        <th>Position ID</th>
        <th>Action</th>
    </tr>
    <% datalist.forEach(user => { %>
    <tr>
        <td><%= user.email %></td>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
        <td><%= user.address %></td>
        <td><%= user.phoneNumber %></td>
        <td><%= user.gender %></td>
        <td><%= user.image %></td>
        <td><%= user.roleId %></td>
        <td><%= user.positionId %></td>
        <td><a href="/edit-crud?id=<%= user.id %>">Edit</a> | <a href="/delete-crud?id=<%= user.id %>" onclick="return confirm('Are you sure?')">Delete</a></td>
    </tr>
    <% }) %>
</table>